<div class="container-fluid">
  <div class="panel">
    <h3><%= current_user.first_name %> <%= current_user.last_name %></h3>
  </div>
  <div class="panel dash-create-stu">
    <form class="form-inline">
      <div class="form-group">
        <%= form_for @student, class:"form-group" do |f|%>
          <%= f.label :name %>
          <%= f.text_field :name, class:"form-control" %>
          <%= f.label :username %>
          <%= f.text_field :username, class:"form-control"%>
          <%= f.label :language %>
          <%= f.text_field :language, class:"form-control" %>
          <%= f.label :level %>
          <%= f.select(:level, [[1, 1], [2, 2], [3, 3]]) %>
          <%= f.label "password image" %>
          <%= f.collection_select(:pass_img_id, @pass_imgs, :id, :img, include_blank: true) %>
          <p><button type="submit" class="btn btn-default">Add Student</button></p>
        <% end  %>
      </div>
    </form>
  </div>
  <div class="panel">
    <h3>Students:</h3>
    <%# current_students.each do |student| %>
      <div class="row" id="tch-dash-students">

      </div>
        <!-- <div class="col-md-2"><%#= student.username %></div>
        <div class="col-md-2"><%#= student.name %></div>
        <div class="col-md-2"><%#= student.language %></div>
        <div class="col-md-2"><%#= student.level %></div>
        <div class="col-md-2"><%#= image_tag(student.pass_img.img) %></div> -->
      </div>
    <%# end  %>
  </div>
</div>
